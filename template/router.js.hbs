import Vue from 'vue'
import Router from 'vue-router'

Vue.use(Router)

export default new Router({
    mode: 'history',
    base: process.env.BASE_URL,
    routes: [
        {{#each list}}
        {
            path: '/{{name}}',
            name: '{{name}}',
            component: () => import('./views/{{file}}')
        },
        {{/each}}
    ]
})